{
  "name": "lumen-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=apps/server\" \"npm run dev --workspace=apps/web\""
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "typescript": "^5.3.3"
  }
}
